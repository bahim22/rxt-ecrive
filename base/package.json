		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^6.2.2",
		"tailwindcss": "^3.0.23"
	},
	"devDependencies": {
		"@babel/core": "^7.17.5",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"autoprefixer": "^10.4.2",
		"babel-jest": "^27.5.1",
		"babel-loader": "^8.2.3",
		"clean-webpack-plugin": "^4.0.0",
