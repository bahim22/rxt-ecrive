{
	"endOfLine": "lf",
	"printWidth": 100,
	"semi": false,
	"tabWidth": 2,
	"useTabs": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"trailingComma": "es5",
	"bracketSameLine": true,
	"embeddedLanguageFormatting": "off",
	"htmlWhitespaceSensitivity": "css",
	"quoteProps": "as-needed",
	"overrides": [
		{
			"files": ["*.json"],
			"options": {
				"parser": "json"
			}
		},
		{
			"files": ["*.ts"],
			"options": {
					"parser": "typescript"
			}
		},
		{
			"files": ["*.tsx"],
			"options": {
					"parser": "typescript",
					"jsxBracketSameLine": true,
					"jsxSingleQuote": true
				}
		}
	]
}
