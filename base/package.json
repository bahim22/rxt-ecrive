{
	"name": "react-portfo2",
	"version": "3.3.2",
	"main": "index.js",
	"license": "MIT",
	"description": "React portfolio for personal coding projects",
	"scripts": {
		"lint:pretest": "eslint \"src/**/*.{js,jsx}\"",
		"lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
		"test:espretty": "npx eslint-config-prettier App.js",
		"test:lint": "eslint \"src/**/*.{js,jsx}\"",
		"test:webpack": "npx webpack --configtest webpack.dev.config.js",
		"test:webpackpro": "npx webpack --configtest webpack.prod.config.js",
		"prettier:check": "prettier --check \"src/**/*.{js,jsx}\"",
		"prettier:write": "npx prettier --write \"src/**/*.{js,jsx}\"",
		"build": "webpack --config webpack.prod.config.js --mode production",
		"build-bash": "NODE_ENV=production webpack --config webpack.prod.config.js --mode production",
		"dev": "webpack serve --config webpack.dev.config.js --mode development",
		"test:jest": "jest",
		"cypress:open": "cypress open"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bahim22/react-portfo2.git"
	},
	"keywords": [
		"React",
		"Webpack",
		"Babel",
		"PostCSS",
		"portfolio",
		"tailwindcss"
	],
	"author": "Hima Balde (Dionysus Era Dev)",
	"bugs": {
		"url": "https://github.com/bahim22/react-portfo2/issues"
	},
	"homepage": "https://github.com/bahim22/react-portfo2#readme",
	"dependencies": {
		"@headlessui/react": "^1.5.0",
